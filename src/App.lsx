import React from 'react'
import logo from './logo.svg'
import lscLogo from './lsclogo.svg'
import './App.css'

class LscLogo extends React.Component:
  render() ->
    <img src={lscLogo} className="App-logo" alt="lsc logo" />

App() ->
  <div className="App">
    <header className="App-header">
      <img src={logo} className="App-logo" alt="logo" />
      <LscLogo />
      <h1 className="App-title">Welcome to React + LightScript</h1>
    </header>
    <p className="App-intro">
      To get started, edit <code>src/App.lsx</code> and save to reload.
    </p>
  </div>

export default App
